<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:ChatBox.ViewModel"
                    xmlns:view="clr-namespace:ChatBox.View">

   

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:UserVM}">
        <view:User />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CalendarVM}">
        <view:Calendar />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:GeminiChatVM}">
        <view:GeminiChat />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:GPTChatVM}">
        <view:GPTChat />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ImageRecpgnitionVM}">
        <view:ImageRecognition />
    </DataTemplate>

</ResourceDictionary>